snippet todo "TODO"
" TODO: ${0}
endsnippet

snippet f "function"
function! ${1}(${2})
  ${3}
endfunction
endsnippet

snippet if "if statement"
if ${1}
  ${0:${VISUAL}}
endif
endsnippet

snippet wh "while loop"
while ${1}
  ${0:${VISUAL}}
endwhile
endsnippet

snippet try "try catch"
try
  ${0}
catch /E31/
endtry
endsnippet
